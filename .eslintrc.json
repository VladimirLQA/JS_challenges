{
  "extends": [
    "prettier",
    "metarhia"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "strict": "off",
    "no-inner-declarations": "off",
    "max-len": [
      "error",
      {
        "ignoreComments": true
      }
    ],
    "camelcase": "off"
  }
}
